@ {
	999 LOADBG GE2018/WipeWithSFX
	# 998 is reserved for the out, so it has time to load
	998 LOADBG GE2018/WipeWithSFX
	300 LOADBG GE2018/DraftBg
	310 MIXER OPACITY 0
	310 CG ADD 0 GE2018/winner4 1 @data
}
@CUE Show {
    999 PLAY
}
@9 999 {
    310 MIXER OPACITY 1
    310 CG NEXT 0
	300 LOOP
}

@RUNCHILDORCUE change End {
	998 PLAY
}

@9 998 {
	300 STOP
	300 CLEAR
	310 CLEAR
	311 CLEAR
}
@END 998 {
    998 CLEAR
}
