<?xml version="1.0" encoding="UTF-8"?>
<scene width="1920" height="1080" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/CasparCG/Server/2.1.0/core/producer/scene/scene.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <variables>
    <variable type="string" public="true" id="candidate_name">FIRST LAST NAME</variable>
    <variable type="string" public="true" id="candidate_position">POSITION POSITION POSIITON</variable>
    <variable type="string" public="true" id="candidate_photo"></variable>

    <!-- common styling numbers -->
    <variable type="number" id="total_width">865</variable>
    <variable type="number" id="box_dim">250</variable>
    <variable type="number" id="box_border">7</variable>
    <variable type="number" id="box_border_angle">45</variable><!-- TODO - this maths isnt quite correct -->
    <variable type="number" id="text_spacing">10</variable>

    <!-- for animating separately -->
    <variable type="number" public="true" id="text_y">0</variable>
    <variable type="number" public="true" id="text_blur">0</variable>
    <variable type="number" public="true" id="text_opacity">1</variable>

  </variables>

  <layers>
    <!-- <layer id="name_mask"> This doesnt work well with blur
      <producer>[COLOR] #ffffff</producer>
      <x>0</x>
      <y>${layer.border.height}</y>
      <width>${variable.total_width}</width>
      <is_key>true</is_key>
    </layer> -->
    <layer id="name">
      <producer>[TEXT] "" 0 0 size 40 color #262626 font HelveticaNeue</producer>
      <x>${layer.border.x + layer.border.width / 2 - layer.name.width / 2}</x>
      <y>${layer.border.y + layer.border.height + variable.text_spacing + layer.name.height - text_y}</y>
      <blur_y>${text_blur}</blur_y>
      <crop>
        <upper_left_y>300</upper_left_y>
      </crop>
      <adjustments>
        <opacity>${text_opacity}</opacity>
      </adjustments>
      <parameters>
        <text>${candidate_name}</text>
      </parameters>
    </layer>
    <!-- <layer id="position_mask"> This doesnt work well with blur
      <producer>[COLOR] #ffffff</producer>
      <x>0</x>
      <y>${layer.border.height}</y>
      <width>${variable.total_width}</width>
      <is_key>true</is_key>
    </layer> -->
    <layer id="position">
      <producer>[TEXT] "" 0 0 size 40 color #262626 font HelveticaNeue-Bold</producer>
      <x>${layer.border.x + layer.border.width / 2 - layer.position.width / 2}</x>
      <y>${layer.name.y + layer.position.height + 3}</y>
      <blur_y>${text_blur}</blur_y>
      <adjustments>
        <opacity>${text_opacity}</opacity>
      </adjustments>
      <parameters>
        <text>${candidate_position}</text>
      </parameters>
    </layer>
    <layer id="border">
      <producer>[COLOR] #262626 #262626 #777877</producer><!-- The light parts wants to be weighted more towards the right -->
      <x>${(variable.total_width - (variable.box_dim + 2 * variable.box_border)) / 2}</x>
      <y>0</y>
      <width>${variable.box_dim + 2 * variable.box_border}</width>
      <height>${variable.box_dim + 2 * variable.box_border}</height>
    </layer>
    <layer id="background">
      <producer>[COLOR] #262626</producer>
      <x>${layer.border.x + variable.box_border}</x>
      <y>${layer.border.y + variable.box_border}</y>
      <width>${variable.box_dim}</width>
      <height>${variable.box_dim}</height>
    </layer>
    <layer id="photo">
      <producer>[PNG_BASE64]</producer>
      <x>${layer.border.x + variable.box_border}</x>
      <y>${layer.border.y + variable.box_border}</y>
      <width>${variable.box_dim}</width>
      <height>${variable.box_dim}</height>
      <parameters>
        <image>${candidate_photo}</image>
      </parameters>
    </layer>
  </layers>

  <marks>
    <mark at="0" type="start" label="intro" />
    <mark at="35" type="stop" />
    <mark at="100" type="start" label="outro" />
    <mark at="135" type="remove"/>
  </marks>
</scene>
